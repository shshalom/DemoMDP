{
  "name": "@mdp-framework/ai-services",
  "version": "1.1.16",
  "description": "Provider-agnostic AI service layer for MDP Framework",
  "type": "module",
  "author": "MDP Framework Contributors",
  "homepage": "https://github.com/mdp-labs/mdp-mlang#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdp-labs/mdp-mlang.git",
    "directory": "packages/ai-services"
  },
  "bugs": {
    "url": "https://github.com/mdp-labs/mdp-mlang/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./contracts": {
      "types": "./dist/contracts/index.d.ts",
      "import": "./dist/contracts/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0"
  },
  "peerDependencies": {
    "openai": "^4.75.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "openai": "^4.75.0",
    "@types/node": "^20.14.0",
    "@vitest/coverage-v8": "^1.6.0",
    "typescript": "^5.6.0",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "mdp",
    "ai",
    "llm",
    "anthropic",
    "openai",
    "provider"
  ],
  "license": "MIT"
}
