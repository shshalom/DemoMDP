{
  "name": "@mdp-framework/gateway",
  "version": "1.1.16",
  "description": "MDP Gateway - Policy resolver, action engine, and adapter orchestration",
  "type": "module",
  "author": "MDP Framework Contributors",
  "homepage": "https://github.com/mdp-labs/mdp-mlang#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdp-labs/mdp-mlang.git",
    "directory": "packages/gateway"
  },
  "bugs": {
    "url": "https://github.com/mdp-labs/mdp-mlang/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./store": {
      "types": "./dist/store/index.d.ts",
      "import": "./dist/store/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@mdp-framework/schemas": "^1.1.0",
    "@mdp-framework/adapters": "^1.1.0",
    "@mdp-framework/ai-services": "^1.1.3",
    "pg": "^8.11.0"
  },
  "peerDependencies": {
    "@mdp-framework/schemas": "^1.1.0",
    "@mdp-framework/adapters": "^1.1.0",
    "@mdp-framework/ai-services": "^1.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/pg": "^8.11.0",
    "typescript": "^5.6.0",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "mdp",
    "gateway",
    "resolver",
    "action-engine"
  ],
  "license": "MIT"
}
