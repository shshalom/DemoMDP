{
  "name": "@mdp-framework/mlang",
  "version": "1.1.17",
  "description": "MLang CLI - Policy compiler, PR governance, and developer tools",
  "type": "module",
  "author": "MDP Framework Contributors",
  "homepage": "https://github.com/mdp-labs/mdp-mlang#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdp-labs/mdp-mlang.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/mdp-labs/mdp-mlang/issues"
  },
  "bin": {
    "mlang": "./bin/mlang.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "@mdp-framework/schemas": "^1.1.16",
    "@mdp-framework/adapters": "^1.1.16",
    "@mdp-framework/gateway": "^1.1.16",
    "@mdp-framework/mcp-server": "^1.1.16",
    "@mdp-framework/agents": "^1.1.16",
    "@mdp-framework/ai-services": "^1.1.16",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "yaml": "^2.3.4",
    "gray-matter": "^4.0.3",
    "@noble/ed25519": "^2.0.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "typescript": "^5.6.0",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "mlang",
    "policy",
    "governance",
    "cli"
  ],
  "license": "MIT"
}
